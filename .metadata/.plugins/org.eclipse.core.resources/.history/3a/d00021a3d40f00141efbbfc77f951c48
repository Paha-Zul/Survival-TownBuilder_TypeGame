	public Residence(Entity owner, String name, int type, boolean active,
			int buildingType) {
		super(owner, name, type, active, buildingType);
		// TODO Auto-generated constructor stub
	}

	public Residence(Entity owner, int buildingType) {
		super(owner, buildingType);
		// TODO Auto-generated constructor stub
	}
	
	public Residence(Entity owner) {
		super(owner, Constants.BUILDING_RESIDENCE);
		// TODO Auto-generated constructor stub
	}

}
